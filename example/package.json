{
  "name": "@jtdc/example",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run jtdc",
    "jtdc": "jtdc --rootDir ./src --includes '*.json' --outDir ./src/gen --typeGuards",
    "start": "ts-node ./src/index.ts",
    "clean": "rimraf ./src/gen ./target"
  },
  "devDependencies": {
    "@jtdc/cli": "^1.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1"
  }
}
